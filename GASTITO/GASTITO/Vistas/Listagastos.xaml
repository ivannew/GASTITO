             x:Class="GASTITO.Vistas.Listagastos"
             NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="lISTAS"
              />
            
            
        </StackLayout>


        <Button Text="GASTO"
                Grid.Row="1"
